<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>

    <% users.forEach(user=> { %>
        <div class="col mb-4">
           <div class="card h-100">
              <div class="card-body">
                 <h5 class="card-title">
                    <%= user.fullName %>
                 </h5>
                    <h4>
                        <%= user.email %>
                    </h4>
                    <p>
                        <%= user.role %>
                    </p>

                 
              </div>
           </div>
        </div>
        <% }) %>
</body>
</html>